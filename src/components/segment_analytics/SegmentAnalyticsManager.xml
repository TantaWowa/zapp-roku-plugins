<?xml version="1.0" encoding="UTF-8"?>
<component name="SegmentAnalyticsManager" extends="Node" >
  
  <interface>
  <!-- This object is a proxy to the SegmentAnalyticsManager api, so that we don't have to instantiate their sdk for each and every call, and also allows for us to decouple from appcontroller/appservices. -->
      <field id="isEnabled" type = "boolean" />
      <function name="identify" />
      <function name="unidentify" />
      <function name="trackEvent" />
      <function name="track" />
      <function name="screen" />
      <function name="group" />
      <function name="getDefaultOptions" />
      <function name="getDefaultProperties" />
  </interface>
</component>
